{
  "mcpServers": {
    "xcodebuild": {
      "command": "npx",
      "args": [
        "-y",
        "@cameroncooke/xcodebuild-mcp"
      ],
      "disabled": false
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@anthropic-ai/mcp-server-memory"
      ],
      "disabled": true
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@anthropic-ai/mcp-server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "<DEIN_GITHUB_TOKEN>"
      },
      "disabled": true
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@anthropic-ai/mcp-server-filesystem",
        "."
      ],
      "disabled": false
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@anthropic-ai/mcp-server-context7"
      ],
      "disabled": true
    }
  }
}
